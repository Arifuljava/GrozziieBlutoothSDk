<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".PrintBluetoothcategory"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                >
                <androidx.cardview.widget.CardView
                    android:id="@+id/dailyCheckCard"
                    android:layout_width="0dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#fff"
                    app:cardUseCompatPadding="true"
                    app:cardElevation="4dp"
                    android:onClick="image_Print"
                    app:cardCornerRadius="2dp"
                    android:layout_marginLeft="0dp"
                    android:clickable="true"
                    android:foreground="?attr/selectableItemBackground"
                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <com.airbnb.lottie.LottieAnimationView
                            android:foregroundTint="@color/white"
                            android:layout_width="50dp"
                            android:id="@+id/animationone"
                            android:layout_marginTop="20dp"
                            android:layout_alignParentRight="false"
                            android:layout_height="@dimen/space_50dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerInParent="false"
                            app:lottie_fileName="picc.json"
                            app:lottie_loop="true"
                            app:lottie_autoPlay="true"
                            android:layout_gravity="center" />


                        <com.nitish.typewriterview.TypeWriterView
                            android:layout_below="@id/animationone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="false"
                            android:padding="8dp"
                            android:layout_marginLeft="0dp"
                            android:textSize="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/material_drawer_header_selection_subtext"
                            android:fontFamily="@font/montserrat"
                            android:text="Image"
                            android:id="@+id/imageprint"
                            android:gravity="center_horizontal" />

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/luckySpinCard"
                    android:layout_width="0dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"
                    android:onClick="pdf_print"

                    app:cardBackgroundColor="#fff"
                    app:cardUseCompatPadding="true"
                    app:cardElevation="4dp"
                    app:cardCornerRadius="2dp"
                    android:layout_marginLeft="0dp"
                    android:clickable="true"
                    android:foreground="?attr/selectableItemBackground">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <com.airbnb.lottie.LottieAnimationView
                            android:foregroundTint="@color/white"
                            android:layout_width="50dp"
                            android:id="@+id/image1"
                            android:layout_marginTop="20dp"
                            android:layout_alignParentRight="false"
                            android:layout_height="@dimen/space_50dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerInParent="false"
                            app:lottie_fileName="pdf.json"
                            app:lottie_loop="true"
                            app:lottie_autoPlay="true"
                            android:layout_gravity="center" />


                        <com.nitish.typewriterview.TypeWriterView
                            android:layout_below="@id/image1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="false"
                            android:padding="8dp"
                            android:layout_marginLeft="0dp"
                            android:textSize="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/material_drawer_header_selection_subtext"
                            android:fontFamily="@font/montserrat"
                            android:text="PDF"
                            android:id="@+id/pdf_print"
                            android:gravity="center_horizontal" />
                    </RelativeLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>
            ///2
            <LinearLayout
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                >
                <androidx.cardview.widget.CardView
                    android:id="@+id/dailyCheckCard_1"
                    android:layout_width="0dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#fff"
                    android:layout_marginBottom="20dp"
                    app:cardUseCompatPadding="true"
                    app:cardElevation="4dp"
                    android:onClick="web_Print"
                    app:cardCornerRadius="2dp"
                    android:layout_marginLeft="0dp"
                    android:clickable="true"
                    android:foreground="?attr/selectableItemBackground"
                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <com.airbnb.lottie.LottieAnimationView
                            android:foregroundTint="@color/white"
                            android:layout_width="50dp"
                            android:id="@+id/deviceanimation"
                            android:layout_marginTop="20dp"
                            android:layout_alignParentRight="false"
                            android:layout_height="@dimen/space_50dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerInParent="false"
                            app:lottie_fileName="docu.json"
                            app:lottie_loop="true"
                            app:lottie_autoPlay="true"
                            android:layout_gravity="center" />


                        <com.nitish.typewriterview.TypeWriterView
                            android:layout_below="@id/deviceanimation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="false"
                            android:padding="8dp"
                            android:layout_marginLeft="0dp"
                            android:textSize="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/material_drawer_header_selection_subtext"
                            android:fontFamily="@font/montserrat"
                            android:text="Label Print"
                            android:id="@+id/web_print"
                            android:gravity="center_horizontal" />

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/luckySpinCard__3"
                    android:layout_width="0dp"
                    android:layout_height="130dp"
                    android:layout_weight="1"
                    android:onClick="historyprint"

                    app:cardBackgroundColor="#fff"
                    app:cardUseCompatPadding="true"
                    app:cardElevation="4dp"
                    app:cardCornerRadius="2dp"
                    android:layout_marginLeft="0dp"
                    android:clickable="true"
                    android:foreground="?attr/selectableItemBackground">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <com.airbnb.lottie.LottieAnimationView
                            android:foregroundTint="@color/white"
                            android:layout_width="50dp"
                            android:id="@+id/ettingsanimation"
                            android:layout_marginTop="20dp"
                            android:layout_alignParentRight="false"
                            android:layout_height="@dimen/space_50dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerInParent="false"
                            app:lottie_fileName="lisst.json"
                            app:lottie_loop="true"
                            app:lottie_autoPlay="true"
                            android:layout_gravity="center" />


                        <com.nitish.typewriterview.TypeWriterView
                            android:layout_below="@id/ettingsanimation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerInParent="false"
                            android:padding="8dp"
                            android:layout_marginLeft="0dp"
                            android:textSize="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/material_drawer_header_selection_subtext"
                            android:fontFamily="@font/montserrat"
                            android:text="Printing List"
                            android:id="@+id/settingstext"
                            android:gravity="center_horizontal" />
                    </RelativeLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>